@startuml

entity "user" {
  email
  password
  admin
}

entity "user_address" {
  post
  tel
  address
}

entity "cash_on_deliver" {
  price
  more_than
}

entity "cash_on_deliver_term" {
  began_at
}

entity "deliver_price" {
  price
  fee
  began_at
}

entity "tax" {
  rate
  began_at
}

entity "cart" {
}

